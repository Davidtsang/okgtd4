<h1>标签</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>


      <th>隶属材料数量</th>



    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= link_to tag.name, tag %></td>


        <td><%= tag.stuffs.count %></td>



      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag', new_tag_path %>
